<view class="appwrapper"><view class="top-bar"><view data-event-opts="{{[['tap',[['backEvent',['$event']]]]]}}" class="left-back iconfont icon-arrow-left _i" bindtap="__e"></view><text class="right-title">详细</text></view><view class="bottom-content"><view data-event-opts="{{[['tap',[['updateInformation',[['o',['mode','avatar'],['pl',userInformation.avatar]]]]]]]}}" class="line line1-avatar" bindtap="__e"><view><text>头像</text><image src="{{userInformation.avatar}}" mode="aspectFill"></image></view><view class="iconfont icon-youjiantou _i"></view></view><view data-event-opts="{{[['tap',[['updateInformation',[['o',['mode','signature'],['pl',userInformation.signature]]]]]]]}}" class="line line2-signature" bindtap="__e"><view><text>签名</text><text>{{userInformation.signature}}</text></view><view class="iconfont icon-youjiantou _i"></view></view><view class="line line3-createAt"><view><text>注册</text><text>{{userInformation.createdAt}}</text></view></view><view data-event-opts="{{[['tap',[['updateInformation',[['o',['mode','nickname'],['pl',userInformation.nickname]]]]]]]}}" class="line line4-nickname" bindtap="__e"><view><text>昵称</text><text>{{userInformation.nickname}}</text></view><view class="iconfont icon-youjiantou _i"></view></view><view data-event-opts="{{[['tap',[['updateInformation',[['o',['mode','gender'],['pl',userInformation.gender]]]]]]]}}" class="line line5-gender" bindtap="__e"><view><text>性别</text><text>{{userInformation.gender===0?'女':'男'}}</text></view><view class="iconfont icon-youjiantou _i"></view></view><view data-event-opts="{{[['tap',[['updateInformation',[['o',['mode','birthday'],['pl',userInformation.birthday]]]]]]]}}" class="line line6-birthday" bindtap="__e"><view><text>生日</text><text>{{userInformation.birthday}}</text></view><view class="iconfont icon-youjiantou _i"></view></view><view data-event-opts="{{[['tap',[['updateInformation',[['o',['mode','phone'],['pl',userInformation.phone]]]]]]]}}" class="line line7-phone" bindtap="__e"><view><text>电话</text><text>{{userInformation.phone}}</text></view><view class="iconfont icon-youjiantou _i"></view></view><view data-event-opts="{{[['tap',[['updateInformation',[['o',['mode','email'],['pl',userInformation.email]]]]]]]}}" class="line line8-email" bindtap="__e"><view><text>邮箱</text><text>{{userInformation.email}}</text></view><view class="iconfont icon-youjiantou _i"></view></view><view class="line line9-logout"><text data-event-opts="{{[['tap',[['toResetPasswordEvent']]]]}}" bindtap="__e">重设密码</text><text data-event-opts="{{[['tap',[['toLoginEvent']]]]}}" bindtap="__e">注销用户</text></view></view></view>